    <script>
        // Prefill Position from URL params
        (function() {
            try {
                const urlParams = new URLSearchParams(window.location.search);
                const jobPositionParam = urlParams.get('position');
                const jobRequirementParam = urlParams.get('exp');
                if (jobPositionParam && document.getElementById('position')) {
                    document.getElementById('position').value = jobPositionParam;
                }
                if (jobRequirementParam && document.getElementById('experience')) {
                    document.getElementById('experience').value = jobRequirementParam;
                }
            } catch (e) {}
        })();

        // Function to show inline messages instead of alerts
        function showMessage(message, type) {
            const messageDiv = $('#form-message');
            messageDiv.removeClass('alert-success alert-danger alert-warning');
            
            if (type === 'success') {
                messageDiv.addClass('alert-success');
            } else if (type === 'error') {
                messageDiv.addClass('alert-danger');
            } else {
                messageDiv.addClass('alert-warning');
            }
            
            messageDiv.html(message.replace(/\n/g, '<br>')).show();
            
            // Scroll to message
            $('html, body').animate({
                scrollTop: messageDiv.offset().top - 100
            }, 500);
        }

        $('form').on('submit', async function(e) {
            e.preventDefault(); // Prevent default form submission
            
            const name = $('#name').val().trim();
            const email = $('#email').val().trim();
            const mobile = $('#mobile').val().trim();
            const experience = $('#experience').val().trim();
            const currentCompany = $('#cur_com').val().trim();
            const currentLocation = $('#cur_job_loc').val().trim();
            const currentCTC = $('#cur_ctc').val().trim();
            const expectedCTC = $('#exp_ctc').val().trim();
            const noticePeriod = $('#notice_period').val().trim();
            const country = $('#country').val();
            const message = $('#about').val().trim();

            // Get position from form field or URL params
            let position = $('#position').val().trim();
            if (!position) {
                try {
                    const urlParams = new URLSearchParams(window.location.search);
                    position = urlParams.get('position') || 'Not specified';
                } catch (e) {
                    position = 'Not specified';
                }
            }

            // Hide any previous messages
            $('#form-message').hide();
            
            // Basic validation - check empty fields
            if (!name || !email || !mobile || !position) {
                showMessage('Please fill in Name, Email, Mobile, and Position fields', 'error');
                return;
            }

            // Email validation
            const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
            if (!email.includes('@')) {
                showMessage('Please include "@" in the email address', 'error');
                return;
            }
            if (!emailRegex.test(email)) {
                showMessage('Please enter a valid email address (e.g., name@example.com)', 'error');
                return;
            }

            // Mobile validation - at least 10 digits
            const mobileDigits = mobile.replace(/\D/g, '');
            if (mobileDigits.length < 10) {
                showMessage('Please enter a valid mobile number (at least 10 digits)', 'error');
                return;
            }

            const formData = {
                position,
                name,
                email,
                mobile,
                experience,
                currentCompany,
                currentLocation,
                currentCTC,
                expectedCTC,
                noticePeriod,
                country,
                message
            };

            // Disable submit button to prevent double submission
            const submitBtn = $(this).find('button[type="submit"]');
            submitBtn.prop('disabled', true).text('Submitting...');

            try {
                // Call the backend API
                const backendUrl = window.location.hostname === 'localhost' ? 'http://localhost:5000' : window.location.origin;
                const response = await fetch(`${backendUrl}/api/apply-job`, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify(formData)
                });

                const result = await response.json();

                if (result.success) {
                    console.log('‚úÖ Application sent successfully:', result);
                    showMessage('üéâ Thank you! Your application has been submitted successfully. Redirecting to careers page...', 'success');
                    $('form')[0].reset(); // Reset form
                    
                    // Redirect to careers page after successful submission
                    setTimeout(() => {
                        window.location.href = '/career';
                    }, 3000);
                } else {
                    throw new Error(result.message || 'Failed to submit application');
                }

            } catch (error) {
                console.error('‚ùå Error sending application:', error);
                showMessage('Failed to submit application. Please try again later.', 'error');
            } finally {
                submitBtn.prop('disabled', false).text('Apply Now');
            }
        });
    </script>
    <!--endbuild-->
</body>

</html>