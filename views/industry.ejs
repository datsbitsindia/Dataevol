        <script>
            // Show the appropriate industry section based on query parameter or clean URL slug
            document.addEventListener('DOMContentLoaded', function () {
                const pathMatch = window.location.pathname.match(/\/industry\/(\w+)/i);
                const slugFromPath = pathMatch ? pathMatch[1].toLowerCase() : null;
                const urlParams = new URLSearchParams(window.location.search);
                const slugFromQuery = (urlParams.get('industry') || '').toLowerCase();
                const fallback = 'healthcare';

                const target = slugFromPath || slugFromQuery || fallback;
                const section = document.getElementById(target);
                if (section) {
                    section.style.display = 'block';
                } else {
                    // If unknown slug, fall back gracefully
                    const fb = document.getElementById(fallback);
                    if (fb) fb.style.display = 'block';
                }
            });
        </script>

